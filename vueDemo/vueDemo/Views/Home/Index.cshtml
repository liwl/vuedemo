<div id="app">
    {{message}}

</div>
<div id="app2">
    <span v-bind:title="message">测试书画报</span>
</div>
<div id="app5">
    <p>{{message1}}</p>
    <button v-on:click="reve">逆转1</button>
</div>
<ol>
    <todo-item></todo-item>

</ol>
<script>
    var app = new Vue({
        el: "#app",
        data: {
            message: 'hello word'
        }

    })
    var app2 = new Vue({
        el: "#app2",
        data: { message: ' 时间加载' + new Date() }
    });
    var app5 = new Vue({
        el: "#app5",
        data: { message1: '中文' },
        methods: { reve: function () { this.message1 = this.message1.split('').reverse().join('') } }
    });
    Vue.component('todo-item', { template: '<li>测试项目</li>' });
</script>
